
@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">
    @await Component.InvokeAsync("_AdminLayoutHeadComponentPartial")


<body>
    <div class="wrapper">
        <!-- Sidebar -->
        @await Component.InvokeAsync("_AdminLayoutSidebarComponentPartial")
        <!-- End Sidebar -->
        @await Component.InvokeAsync("_AdminLayoutNavbarComponentPartial")


        <div class="container">
            <div class="page-inner">
				<div class="page-header">
					<h3 class="fw-bold mb-3">@ViewBag.directory1</h3>
					<ul class="breadcrumbs mb-3">
						<li class="nav-home">
							<a href="#">
								<i class="icon-home"></i>
							</a>
						</li>
						<li class="separator">
							<i class="icon-arrow-right"></i>
						</li>
						<li class="nav-item">
							<a href="#">@ViewBag.directory2</a>
						</li>
						<li class="separator">
							<i class="icon-arrow-right"></i>
						</li>
						<li class="nav-item">
							<a href="#">@ViewBag.directory3</a>
						</li>
					</ul>
				</div>
				@RenderBody()
				</<div>
				</div>

			</div>
        @await Component.InvokeAsync("_AdminLayoutFooterComponentPartial")
            @await Component.InvokeAsync("_AdminLayoutScriptComponentPartial")

    </div>

    </div>
    

   
</body>
</html>

